FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      B0;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0 0 20);

boundaryField
{
    topWall
   {
        type            zeroGradient;
    }
    botWall
  {
        type            zeroGradient;
    }
    frontWall
  {
        type            zeroGradient;
    }
    backWall
  {
        type            zeroGradient;
    }
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }      
}

